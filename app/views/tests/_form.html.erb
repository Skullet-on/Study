<div class="panel panel-default devise-bs">
  <div class="panel-heading">
  </div>
  <div class="panel-body">
    <%= form_for(@test) do |f| %>
      <div class="form-group">
        <%= f.label :subject %><br>
        <%= f.text_field :subject, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :part %><br>
        <%= f.select(:part, options_for_select(1..4)) %>
      </div>
      <div class="actions">
        <%= f.submit %>
      </div>
    <% end %>
  </div>
</div>